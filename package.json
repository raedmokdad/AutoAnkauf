{
  "name": "autohandel-seite",
  "version": "1.0.0",
  "description": "Professionelle Auto-Verkaufsseite",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "postbuild": "react-snap",
    "build:simple": "vite build",
    "preview": "vite preview",
    "convert-csv": "node scripts/csv-to-json.js"
  },
  "reactSnap": {
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ],
    "source": "dist",
    "minifyHtml": {
      "collapseWhitespace": false,
      "removeComments": false
    },
    "crawl": false,
    "skipThirdPartyRequests": true,
    "include": [
      "/",
      "/bewertung",
      "/bewertung-komplett",
      "/ankauf",
      "/angebot",
      "/ueber-uns",
      "/faq",
      "/kontakt",
      "/impressum",
      "/datenschutz",
      "/marken/bmw-verkaufen",
      "/marken/mercedes-verkaufen",
      "/marken/volkswagen-verkaufen",
      "/marken/audi-verkaufen",
      "/marken/opel-verkaufen",
      "/marken/ford-verkaufen",
      "/marken/skoda-verkaufen",
      "/marken/renault-verkaufen",
      "/marken/seat-verkaufen",
      "/marken/toyota-verkaufen",
      "/ratgeber/auto-verkaufen-checkliste",
      "/ratgeber/kaufvertrag-auto-muster",
      "/ratgeber/fahrzeugbewertung-ablauf",
      "/ratgeber/auto-ohne-tuev-verkaufen",
      "/ratgeber/unfallwagen-verkaufen",
      "/ratgeber/export-auto-verkaufen",
      "/ratgeber/gebrauchtwagen-preise",
      "/ratgeber/auto-abmelden-verkauf"
    ]
  },
  "dependencies": {
    "nodemailer": "^6.9.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-google-recaptcha": "^3.1.0",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^6.30.2"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "react-snap": "^1.23.0",
    "terser": "^5.46.0",
    "vite": "^5.0.8"
  }
}
